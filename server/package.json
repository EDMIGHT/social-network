{
  "name": "server",
  "version": "1.0.0",
  "description": "Api for web application: social",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "swagger": "tsx src/swagger.ts",
    "build": "prisma generate && webpack --mode production",
    "start": "node dist/app.js",
    "dev": "concurrently \"webpack --watch --mode development\" \"wait-on dist/app.js && nodemon dist/app.js\"",
    "migrate": "npx prisma migrate dev",
    "push": "npx prisma db push",
    "gen": "npx prisma generate",
    "studio": "npx prisma studio",
    "seed": "npx prisma db seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:write": "prettier --write \"**/*.{js,jsx}\" --cache",
    "format:check": "prettier --check \"**/*.{js,jsx}\" --cache"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
